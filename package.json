{
  "name": "vscode-levels",
  "displayName": "vscode-levels",
  "description": "JavaScript scope-based syntax highlighting",
  "version": "0.1.0",
  "publisher": "lucasazzola",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "configuration": {
      "title": "levels configuration",
      "properties": {
        "levels.colors": {
          "type": "array",
          "default": [
            "#fdf6e3",
            "#fafa99",
            "#a9f2ff",
            "#faadba",
            "#c9bfff",
            "#f9b5ff",
            "#c7c7c7",
            "#ffc47d",
            "#cf9369"
          ],
          "description": "The text colors to use for level highlighting"
        }
      }
    }
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/esprima": "^2.1.33",
    "@types/lodash": "^4.14.63",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "conventional-changelog-cli": "^1.3.1",
    "mocha": "^2.3.3",
    "npm-run-all": "^4.0.2",
    "typescript": "^2.2.2",
    "vscode": "^1.0.0"
  },
  "repository": "DerFlatulator/vscode-levels",
  "dependencies": {
    "eslevels": "^0.4.0",
    "esprima": "^3.1.3",
    "lodash": "^4.17.4"
  }
}
